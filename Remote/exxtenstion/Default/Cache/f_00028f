						$("#tabmenu li.li-3 a").addClass('current');
					}else if(vnconnect == '4'){
						$('#sc2').show();
						$("#tabmenu li a").removeClass('current');
						$("#tabmenu li.li-2 a").addClass('current');
					}
				}else if(service == '2'){
					if(vnconnect == '1'){
						$('#sc2').show();
						$("#tabmenu li a").removeClass('current');
						$("#tabmenu li.li-2 a").addClass('current');
					}else{
						$('#sc1').show();
					}
				}else if(service == '3'){
					if(vnconnect == '2'){
						$('#sc3').show();
						$("#tabmenu li a").removeClass('current');
						$("#tabmenu li.li-3 a").addClass('current');
					}else if(vnconnect == '4'){
						$('#sc2').show();
						$("#tabmenu li a").removeClass('current');
						$("#tabmenu li.li-2 a").addClass('current');
					}else{
						$('#sc1').show();
					}
				}else{
					$('#sc1').show();
				}
			}, 1000);
			
			if(vnconnect == '2' && (service == '0' || service == '3')){
				$('.submenuEpay li:nth-child(2)').addClass('li-active');
