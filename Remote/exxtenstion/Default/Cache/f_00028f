	var emailAjax;

	function resizeTranFrame(data) {
		var frameHeightPx = data.bodyHeight;
		$("#tranFrame").height(($("#tranFrame").height() + frameHeightPx) + "px");
	}

	function resizeIframe() {
		var $iframes = $("iframe.autoHeight");
		$iframes.each(function() {
			var iframe = this;
			$(iframe).load(function() {
				setHeight(iframe);
			});
		});
	}

	function forceExit() {
		window.location.replace('/etaxnnt/Request?&dse_sessionId=3Av1-IRbSfVx6STm51zk3zr&dse_applicationId=-1&dse_pageId=9&dse_operationName=corpUserLogoutProc&dse_errorPage=error_page.jsp&dse_processorState=initial&dse_nextEventName=start');
	}

	function lout() {
		var confirmLogout = window.confirm('Quý khách có muốn đăng xuất khỏi dịch vụ?');
		if(confirmLogout){
			window.returnMessage=null;
		}
		return confirmLogout;
	}

	$(document).ready(function() {

		//Tu dong load
		emailAjax = setInterval(function() {
			getEmail();
		}, 1200000);
		$('.submenuEpay li').first().addClass('li-active');
		$('#tranFrame').iframeAutoHeight({minHeight: 560});
	});
	
	function myFunction() {
		jQuery.ajax({
			url : "/etaxnnt/Request?&dse_sessionId=3Av1-IRbSfVx6STm51zk3zr&dse_applicationId=-1&dse_pageId=9&dse_operationName=corpUserLogoutProc&dse_processorState=initial&dse_nextEventName=logout",
			type : "post",
			data : "",
			cache : false,
			success : function(html) {
			},
			error : function() {
			}
		});
 		//console.log("function called");
  		return "Write something clever here...";
	}  
    
    window.onbeforeunload = function(e) {
        if(window.returnMessage.length > 0) {
        	myFunction();
       	}
    	//return window.returnMessage;
    }   
      
	document.onkeydown = KeyCheck;
	window.returnMessage="You are now logged out.";
	function KeyCheck(e) {
    	var key = (window.event) ? event.keyCode : e.keyCode;
    	//alert(key);
    	if(key==116) {
    		window.returnMessage=null;
    	}
    }

	function getEmail() {
		jQuery.ajax({
			url : "/etaxnnt/Request?&dse_sessionId=3Av1-IRbSfVx6STm51zk3zr&dse_applicationId=-1&dse_pageId=9&dse_operationName=corpInternalMessageProc&dse_processorState=initial&dse_nextEventName=count",
			type : "post",
			data : "",
			cache : false,
			success : function(html) {
				var messageCount = $.trim(html);
				var countHtml = "(" + messageCount + ")";

				if (messageCount > 0) {
					$('#emailNew').css('display', '');
					$('#email').css('display', 'none');
					$('#msgCount').html(countHtml);
				} else {
					$('#emailNew').css('display', 'none');
					$('#email').css('display', '');
				}

			},
			error : function() {
				clearInterval(emailAjax);
			}
		});
	}

	function changeTinDLT() {
		window.returnMessage=null;
		var tin_dlt = document.getElementById("tin_dlt").value;
		var url = '/etaxnnt/Request?&dse_sessionId=3Av1-IRbSfVx6STm51zk3zr&dse_applicationId=-1&dse_pageId=9&dse_operationName=corporateHomeProc&dse_processorState=initial&dse_nextEventName=reLoad'+"&tin_dlt="+tin_dlt;
		document.location.href=url;
	}
	function active(e){
		$('.tabcontent ul.submenuEpay li').removeClass('li-active');
		$('.sub-menu-lvl3 li').removeClass('li-active-lvl3');
		$(e).addClass('li-active');
	}
	function activeChild(e){
		var className ="."+$(e).attr('class');
		$('.tabcontent ul.submenuEpay li').removeClass('li-active');
		$('.sub-menu-lvl3 li').removeClass('li-active-lvl3');
		$(className+"lvl2 > li").first().addClass('li-active');

	}
	function activeLiLvl3(e){
		$('.tabcontent ul.submenuEpay li').removeClass('li-active');
		$('.sub-menu-lvl3 li').removeClass('li-active-lvl3');
		setTimeout(function(){
			$(e).addClass('li-active-lvl3');
		}, 500);
		$(e).parent().parent().addClass('li-active');
	}
	//ANHLD: disable F5 and back button
	// slight update to account for browsers not supporting e.which
	function disableF5(e) { if ((e.which || e.keyCode) == 116) e.preventDefault(); };
	// To disable f5
	    /* jQuery < 1.7 */
	$(document).bind("keydown", disableF5);
	/* OR jQuery >= 1.7 */
	$(document).on("keydown", disableF5);

	// To re-enable f5
	    /* jQuery < 1.7 */
	$(document).unbind("keydown", disableF5);
	/* OR jQuery >= 1.7 */
	$(document).off("keydown", disableF5);
</script>

