	$(document).ready(function() {
	    $('#tin_dlt').select2();
	    
	    var vnconnect = '';
	    var _vnconnect = '';
	    var service = '';
	    
	    //console.log("_vnconnect = "+_vnconnect);
	    //console.log("service = "+service);
	    
	    if(vnconnect == '1' || vnconnect == '2' || vnconnect == '4'){
	    	setTimeout(function(){
				$('#sc1').hide();
				if(service == '0'){
					if(vnconnect == '1'){
						$('#sc4').show();
						$("#tabmenu li a").removeClass('current');
						$("#tabmenu li.li-4 a").addClass('current');
					}else if(vnconnect == '2'){
						$('#sc3').show();
