function loadDsach_Loi() {
    $("#ctiet_loi").dialog({
        autoOpen: false,
        show: "blind",
        modal: true,
        position: ['center', 100],
        height: 350,
        width: 650,
        dialogClass: "ipad",
        hide: "explode",
        beforeClose: function() {},
        buttons: function() {
            $(this).dialog('close');
        }
    });
};

function query_loi(id) {
    $("#ctiet_loi").dialog("open");
    jQuery.ajax({
        url: "/etaxnnt/Request?dse_sessionId=NvWohSWZSY-slJerugIn8vY&dse_applicationId=-1&dse_operationName=traCuuToKhaiProc&dse_pageId=11&dse_processorState=viewTraCuuTkhai&dse_processorId=ASCNHAGZCOIQENJGDUJPFIFBHTBPIRHFELFPERBC&dse_nextEventName=query_loi",
        type: "post",
        data: "ma_loi=" + id,
        cache: false,
        success: function(html) {
            jQuery("#divCTL").html(html);
        }
    });
}

function checkdate(date) {
    var loai;
    var nam = date.split("/")[2];
    if (nam < 2011) {
        loai = 1;
    }
    return loai;
}

function showDatetime() {

    $(".dateMSB").datepicker({
        buttonImage: 'static/images/default/button_date.gif',
        buttonImageOnly: true,
        changeYear: true,
        changeMonth: true,
        dateFormat: "dd/mm/yy",
        showOn: 'button',
        constrainInput: false
    });
    /* $(".dateMSB").attr("placeholder", "dd/MM/yyyy"); */

    if ($('.time') != null && $('.time').length) {
        jQuery(".time").mask("99:99:99");
        $(".time").attr("placeholder", "HH:mm:ss");
    }
}

function formatDate(strDate) {
    var dateParts = strDate.split("/");
    var date = new Date(dateParts[2], (dateParts[1] - 1), dateParts[0]);
    return Date.parse(date);
}


function validateForm() {
    var startDate = document.getElementById("qryFromDate").value;
    var endDate = document.getElementById("qryToDate").value;
//     if ($.trim(startDate) != "" && $.trim(endDate) == "") {
//         alert("Bạn phải nhập trường đến ngày");
//         return false;
//     } else 
	if ($.trim(startDate) == "") {
        alert("Bạn phải nhập trường từ ngày");
        return false;
    }
	if ($.trim(endDate) == "") {
        alert("Bạn phải nhập trường đến ngày");
        return false;
    }
    var startYear = startDate.substring(6);
    var endYear = endDate.substring(6);
    var startMonth = startDate.substring(3, 5);
    var endMonth = endDate.substring(3, 5);
    var startDay = startDate.substring(0, 2);
    var endDay = endDate.substring(0, 2);
    var newStartDate = startYear + startMonth + startDay;
    var newEndDate = endYear + endMonth + endDay;
    var timeInMs = new Date().valueOf();
    var jStartDate = formatDate($("#qryFromDate").val());
    var jEndDate = formatDate($("#qryToDate").val());
    if(startDate != null && startDate != "" && endDate != null && endDate != "") {
	    if (newStartDate > newEndDate) {
	        alert('Từ ngày không được lớn hơn đến ngày.');
	        $("#qryFromDate").focus();
	        return false;
	    } else if (newEndDate - newStartDate > 10000) {
	        alert('Khoảng thời gian không lớn hơn 1 năm.');
	        return false;
	    } 
    }
    if (timeInMs < jStartDate) {
	        alert("Từ ngày không được lớn hơn ngày hiện tại.");
	        $("#qryFromDate").focus();
	        return false;
	} else if (timeInMs < jEndDate) {
	        alert("Đến ngày không được lớn hơn ngày hiện tại.");
	        $("#qryToDate").focus();
	        return false;
	}

    if (!ktraKyTThue()) {
        return false;
    }
    document.traCuuKhaiForm.dse_nextEventName.value = "query";
    $("#traCuuKhaiForm").submit();
    return true;
}

function backPage() {
    document.traCuuKhaiForm.dse_nextEventName.value = "back";
    $("#traCuuKhaiForm").submit();
    return true;
}


