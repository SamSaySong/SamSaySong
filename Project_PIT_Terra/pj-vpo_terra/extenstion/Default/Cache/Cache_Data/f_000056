<script type="text/javascript">

var tieudelon = document.getElementById("tieudelon");
    tieudelon.innerHTML =
'<span class="icon_tt">'+
'<img height="22px" width="22px" src="/etaxnnt/static/images/epay/logo.png">'+
'</span>'+
'<i>'+
'<span class="text_tt">'+
'Tra cứu &gt;&nbsp'+
'<span class="red_b">Tờ khai</span>'+
'</span>'+
'</i>'; 


var maTKhaiObj = document.getElementById('maTKhai');
var tdKyKKhaiTuKyObj = document.getElementById('tdKyKKhaiTuKy');
var tdKyKKhaiDenKyObj = document.getElementById('tdKyKKhaiDenKy');
var tdKyKKhaiTuKyLabelObj = document.getElementById('tdKyKKhaiTuKyLabel');
var tdKyKKhaiDenKyLabelObj = document.getElementById('tdKyKKhaiDenKyLabel');

function changeKyKKhai(selectObj){
//    resetText();
    var maTKhai = selectObj.value;
    var kieuKy = document.getElementById('kieuKy').value;
    var kieuKyObj = document.getElementById('kieuKy');
    var zeturn;
    if('--' == maTKhai){
        tdKyKKhaiTuKyObj.innerHTML = '';
        tdKyKKhaiDenKyObj.innerHTML = '';
        tdKyKKhaiTuKyLabelObj.innerHTML = '';
        tdKyKKhaiDenKyLabelObj.innerHTML = '';
        selectObj.selectedIndex = selectObj.selectedIndex + 1;
        changeKyKKhai(selectObj);
        zeturn = 'tatca';
    }else if('00' == maTKhai){
        tdKyKKhaiTuKyObj.innerHTML = '';;
        tdKyKKhaiDenKyObj.innerHTML = '';
        tdKyKKhaiTuKyLabelObj.innerHTML = '';
        tdKyKKhaiDenKyLabelObj.innerHTML = '';
        zeturn = '00';
    }else{
        kieuKy = arrTKhai[maTKhai].kieuKy;
        if ('D' == kieuKy) {
            tdKyKKhaiTuKyObj.innerHTML = '';;
            tdKyKKhaiDenKyObj.innerHTML = '';
            tdKyKKhaiTuKyLabelObj.innerHTML = '';
            tdKyKKhaiDenKyLabelObj.innerHTML = '';
            zeturn = 'D';
        } else {
            tdKyKKhaiTuKyLabelObj.innerHTML = 'Kỳ tính thuế từ kỳ';
            tdKyKKhaiDenKyLabelObj.innerHTML = 'Đến kỳ';
            if('M' == kieuKy){
                tdKyKKhaiTuKyObj.innerHTML = tdKyKKhaiTuKyMHTML;
                tdKyKKhaiDenKyObj.innerHTML = tdKyKKhaiDenKyMHTML;
                zeturn = 'M';
            }else if('Q' == kieuKy){
                tdKyKKhaiTuKyObj.innerHTML = tdKyKKhaiTuKyQHTML;
                tdKyKKhaiDenKyObj.innerHTML = tdKyKKhaiDenKyQHTML;
                zeturn = 'Q';
            }else if('Y' == kieuKy){
                tdKyKKhaiTuKyObj.innerHTML = tdKyKKhaiTuKyYHTML;
                tdKyKKhaiDenKyObj.innerHTML = tdKyKKhaiDenKyYHTML;
                zeturn = 'Y';
            }
        }
        kieuKyObj.value = kieuKy;
    }
    return zeturn;
}
changeKyKKhai(maTKhaiObj);

function ktraKyTThue(){
    var msg = 'Kỳ tính thuế từ kỳ không được lớn hơn kỳ tính thuế đến kỳ';

    var kieuKy = document.getElementById('kieuKy').value;

    var kyKKhaiTuKyYObj = document.getElementById('kyKKhaiTuKyY1');  
    var kyKKhaiDenKyYObj = document.getElementById('kyKKhaiDenKyY1');   
    if(kyKKhaiTuKyYObj != null && kyKKhaiDenKyYObj != null){
        var kyKKhaiTuKyY = toNumber(kyKKhaiTuKyYObj.value);    
        var kyKKhaiDenKyY = toNumber(kyKKhaiDenKyYObj.value);
        if('M' == kieuKy || 'Q' == kieuKy){
            var kyKKhaiTuKyMQObj = document.getElementById('kyKKhaiTuKyMQ1');  
            var kyKKhaiDenKyMQObj = document.getElementById('kyKKhaiDenKyMQ1'); 
            var kyKKhaiTuKyMQ = toNumber(kyKKhaiTuKyMQObj.value);
            var kyKKhaiDenKyMQ = toNumber(kyKKhaiDenKyMQObj.value);
            if(kyKKhaiTuKyY > kyKKhaiDenKyY){
                alert(msg);     
                kyKKhaiTuKyYObj.select();
                return false;
            }else if(kyKKhaiTuKyY == kyKKhaiDenKyY){
                if(kyKKhaiTuKyMQ > kyKKhaiDenKyMQ){                
                    alert(msg);     
                    kyKKhaiTuKyMQObj.focus();
                    return false;
                }
            }
        }    
        if('Y' == kieuKy){
            if(kyKKhaiTuKyY > kyKKhaiDenKyY){
                alert(msg);     
                kyKKhaiTuKyYObj.select();
                return false;
            }
        }
    }
    return true;
} 

function isNumber(evt) {
    evt = (evt) ? evt : window.event;
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 ||  57 < charCode)) {
        return false;
    }
    return true;
}
</script>
